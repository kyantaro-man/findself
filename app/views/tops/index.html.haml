  %footer
    .footer__inner
      %ul.footer__menu
        %li.footer__menu__column
          %p.footer__menu__title About
          %ul.footer__menu__list
            %li.footer__menu__item
              = link_to "運営者", "#", class: "link white"
            %li.footer__menu__item
